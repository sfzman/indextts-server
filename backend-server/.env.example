APP_ENV=local

# 服务器配置
SERVER_PORT=8080
GIN_MODE=debug

# 数据库配置
# MySQL 配置（当 DB_DRIVER=mysql 时使用）
DB_DRIVER=mysql
DB_HOST=localhost
DB_PORT=3306
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_NAME=indextts

# 阿里云 OSS 配置
OSS_ENDPOINT=oss-cn-beijing.aliyuncs.com
OSS_ACCESS_KEY_ID=your_oss_access_key_id
OSS_ACCESS_KEY_SECRET=your_oss_access_key_secret
OSS_BUCKET_NAME=your_bucket_name

# CORS 跨域配置
CORS_ORIGINS=*

# 推理服务配置
INFERENCE_URL=https://your-inference-service-url.com

# JWT 私钥 (RS256, PEM 格式, 用于调用推理服务)
JWT_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYour RSA private key here\n-----END PRIVATE KEY-----"
JWT_EXPIRE_SECONDS=60
